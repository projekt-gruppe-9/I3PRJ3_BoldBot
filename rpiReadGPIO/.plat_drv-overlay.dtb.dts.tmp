/dts-v1/;
/plugin/;
/ {
  compatible = "brcm,bcm2835", "brcm,bcm2836", "brcm,bcm2708", "brcm,bcm2709";
  fragment@0 {

    target-path = "/";
    __overlay__ {

      plat_drv: plat_drv@0 {

        gpios = <&gpio 17 0>;
        mydevt-custom = <0x12345678>;
        compatible = "ase, plat_drv";
        status = "okay";
      };
    };
  };
};
